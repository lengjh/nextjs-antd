{
  "name": "hello-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "commit": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "serve": "next build && next export && serve out/"
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.22.2",
    "less": "^3.10.3",
    "next": "9.0.5",
    "null-loader": "^3.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.6.1",
    "babel-plugin-import": "^1.12.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "git-cz": "^3.2.1",
    "less-loader": "^5.0.0",
    "next-compose-plugins": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
